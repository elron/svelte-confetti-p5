{
	"name": "svelte-confetti-p5",
	"description": "An awesome svelte realistic fullpage confetti animation, that can be customized in creative ways like snow flakes, falling balls and more.",
	"version": "0.1.2",
	"author": "Elron Bucai",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltejs",
		"confetti",
		"confetties",
		"snow flakes",
		"canvas",
		"p5",
		"animation",
		"animated"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/elron/svelte-confetti-p5.git"
	},
	"homepage": "https://svelte-confetti-p5.netlify.app/",
	"bugs": {
		"url": "https://github.com/elron/svelte-confetti-p5/issues"
	},
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@macfja/svelte-expirable": "^1.0.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.21.0",
		"@sveltejs/package": "^2.1.0",
		"publint": "^0.1.13",
		"sass": "^1.63.6",
		"svelte": "^4.0.1",
		"svelte-check": "^3.4.4",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.3.9"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@types/p5": "^1.6.2",
		"p5-svelte": "^3.1.2"
	}
}